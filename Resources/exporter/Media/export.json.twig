{% spaceless %}
{
  "id": "{{ entity.id }}",
  "source": "{{ entity.source }}",
  "reference": "{{ entity.reference }}",
  "referencePrefix": "{{ entity.referencePrefix }}",
  "extension": "{{ entity.extension }}",
  "providerServiceName": "{{ entity.providerServiceName }}",
  "name": "{{ entity.name }}",
  "description": "{{ entity.description }}",
  "size": "{{ entity.size }}",
  "mimeType": "{{ entity.mimeType }}",
  "enabled": "{{ entity.enabled }}",
  "createdAt": "{{ entity.createdAt|date('c') }}",
  "metadata": { {% for k,v in entity.metadata %}{% if not loop.first %}, {% endif %}"{{ k }}": "{{ v }}"{% endfor %} }
}
{% endspaceless %}
