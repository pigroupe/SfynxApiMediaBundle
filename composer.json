{
  "name": "sfynx-project/media-api-bundle",
  "type": "symfony-bundle",
  "description": "This Bundle provides an api to manage medias or documents",
  "keywords": ["media", "api", "rest", "ddd"],
  "homepage": "http://pigroupe.github.io/cmf-sfynx/",
  "license": "LGPL-3.0+",
  "authors": [
    {
      "name": "Etienne de Longeaux",
      "email": "etienne.delongeaux@gmail.com"
    },
    {
      "name": "Sfynx Community",
      "homepage": "https://github.com/pigroupe/SfynxApiMediaBundle/contributors"
    }
  ],

  "require": {
    "php": ">=7.0",
    "ext-imagick":                  "*",
    "symfony/framework-bundle": "~2.8|~3.0",
    "doctrine/orm": "*",
    "doctrine/doctrine-bundle": "*",
    "knplabs/knp-gaufrette-bundle": "~0.4",
    "sfynx-project/core-bundle": "2.x-dev",
    "sfynx-project/tool-crawler-bundle": "2.x-dev"
  },

  "require-dev": {
    "symfony/phpunit-bridge": "~2.7|~3.0",
    "phpunit/phpunit": "*"
  },

  "autoload": {
    "psr-0": { "Sfynx\\ApiMediaBundle": "" }
  },

  "target-dir": "Sfynx/ApiMediaBundle",

  "extra": {
    "branch-alias": {
      "dev-master": "2.x-dev"
    }
  }
}